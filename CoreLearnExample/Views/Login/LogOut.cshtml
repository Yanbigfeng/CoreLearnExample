
@{
    ViewData["Title"] = "LogOut";
}

<h1>验证已失败，session不存在，此为错误页面</h1>

<h3>这里设置信息</h3>
<form asp-action="SetSession" target="iframe_display" onsubmit="setTimeout(function () { window.location.href='/Session/Login'; }, 1000)">

    <div>
        <label>姓名：</label>
        <input name="name" />
    </div>

    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
<iframe id="iframe_display" name="iframe_display" style="display: none;"></iframe>